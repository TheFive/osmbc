if !MIXIN_INCLUDE
  include mixins


h3="Votes & Tags ..."
table.table.table-sm
  tbody
    for vote ,i in votes
      tr
        td
          if params && params.edit
            span(class=vote.iconClass)
          else
            +voteButton(vote,article)
        td=vote.name
        td
          if article.votes && article.votes[vote.name]
            - labelclass = "osmbclabel-collect"
            each user in article.votes[vote.name]
              - if (layout.user.OSMUser === user) labelclass = "osmbclabel-user-self"
              span.badge.small-margin(class=labelclass)=user
        td=vote.description
